			"definition": "",
			"event_id": "dialogic_011",
			"value": ""
		},
		{
			"character": "character-1678926913.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "AWww thanks honey!\nAppreciate that"
		},
		{
			"definition": "1678930059-787",
			"event_id": "dialogic_014",
			"operation": "+",
			"random_upper_limit": 100,
			"set_random": false,
