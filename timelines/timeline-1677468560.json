		{
			"character": "character-1677452675.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "So why are you bothering me?"
		},
		{
			"anchor_id": "anchor-1677628632",
			"event_id": "dialogic_016"
		},
		{
			"event_id": "dialogic_013"
		},
		{
			"choice": "You Hate Me?",
